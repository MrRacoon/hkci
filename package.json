{
  "name": "hkci",
  "version": "1.4.5",
  "description": "Command line interpreter for haskind",
  "main": "index.js",
  "scripts": {
    "deps": "npm install",
    "lint": "eslint ./{src,test}/**/*.js --fix",
    "clean:deps": "rm -rf node_modules",
    "clean:dist": "rm -rf dist",
    "clean": "run-p clean:*",
    "build:dist": "NODE_ENV=production babel --out-dir=dist src",
    "build": "run-s clean:dist build:dist",
    "test:src": "NODE_PATH=$PWD/src mocha test/**/*.js",
    "test:dist": "NODE_PATH=$PWD/dist mocha test/**/*.js",
    "test:watch": "NODE_PATH=$PWD/src mocha test/**/*.js --watch",
    "test": "npm run deps && run-s lint test:src build:dist test:dist",
    "dev": "run-s lint clean:dist build:dist test:dist start",
    "watch": "npm run test:watch",
    "start": "node ./index.js",
    "prepublish": "run-s build"
  },
  "keywords": [
    "haskind",
    "functional",
    "cli",
    "command line",
    "interpreter"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrRacoon/hkci.git"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "bin": {
    "hkci": "bin/hkci"
  },
  "lib": {
    "index": "dist/hkci.js"
  },
  "author": "Erik Sutherland <erik.misu@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MrRacoon/hkci/issues"
  },
  "homepage": "https://github.com/MrRacoon/hkci#readme",
  "dependencies": {
    "babel-runtime": "^6.20.0",
    "haskind": "^0.6.0",
    "node-getopt": "^0.2.3",
    "rc": "^1.1.7",
    "readline-vim": "^0.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "^3.12.2",
    "eslint-plugin-flowtype": "^2.29.1",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.0",
    "should": "^11.1.2"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {}
      ]
    ],
    "plugins": [
      "transform-flow-strip-types",
      "transform-runtime",
      "transform-object-rest-spread",
      [
        "typecheck",
        {
          "disable": {
            "production": true
          }
        }
      ]
    ]
  }
}
